{
  "name": "@ketch-sdk/ketch-web-api",
  "version": "0.0.1",
  "description": "Ketch TypeScript Web API",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "ncc build --minify --license licenses.txt src/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest --runInBand",
    "format": "prettier --write \"**/*.{ts,tsx,yml,yaml}\"",
    "format-check": "prettier --check '**/*.ts'",
    "docs": "typedoc --githubPages true --excludeInternal src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ketch-sdk/ketch-web-api.git"
  },
  "author": "Ketch Kloud, Inc. (https://www.ketch.com/)",
  "license": "MIT",
  "homepage": "https://github.com/ketch-sdk/ketch-web-api",
  "bugs": {
    "url": "https://github.com/ketch-sdk/ketch-web-api/issues"
  },
  "devDependencies": {
    "@jest/globals": "^29.2.1",
    "@ketch-sdk/ketch-types": "^0.2.4",
    "@types/jest": "^29.2.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^14.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.17",
    "typescript": "^4.8.4"
  }
}
